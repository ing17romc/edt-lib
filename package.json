{
  "name": "edt-lib",
  "version": "2.0.5",
  "description": "Front-end library, rebuilt for React",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "author": "ing romc",
  "license": "ISC",
  "homepage": "http://ing17romc.github.io/edt-lib/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ing17romc/edt-lib.git"
  },
  "scripts": {
    "clean": "rm package-lock.json && rm -rf node_modules && npm i --force",
    "build": "babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "sb": "npm run build && storybook build && storybook dev -p 6006",
    "release-sb": "npm run build && rm -rf .gh-pages && storybook build && gh-pages -d storybook-static",
    "publish-npm": "npm version patch && npm run build && npm publish --access public"
  },
  "dependencies": {
    "classnames": "^2.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.27.1",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-backgrounds": "^8.6.14",
    "@storybook/addon-controls": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/testing-react": "^2.0.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  },
  "browserslist": {},
  "files": [
    "dist",
    "src/scss",
    "index.scss"
  ]
}
