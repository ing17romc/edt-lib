{
  "name": "edt-lib",
  "version": "2.12.0",
  "description": "Front-end library, rebuilt for React",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "author": "ing romc",
  "license": "ISC",
  "homepage": "http://ing17romc.github.io/edt-lib/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ing17romc/edt-lib.git"
  },
  "scripts": {
    "clean": "rm package-lock.json && rm -rf node_modules && rm -rf .gh-pages && rm -rf storybook-static && rm -rf dist && npm i --force",
    "build": "babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx",
    "test": "jest",
    "lint:js": "eslint . --ext .ts,.tsx  --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:ts": "npx tsc --noEmit",
    "lint": "npm run lint:js && npm run lint:scss",
    "sb": "npx storybook dev -p 6006",
    "build-sb": "npx storybook build",
    "release-sb": "npm run build-sb && rm -rf .gh-pages && gh-pages -d storybook-static",
    "publish-npm-patch": "npm version patch && npm run build && npm publish --access public",
    "publish-npm-minor": "npm version minor && npm run build && npm publish --access public",
    "publish-npm-major": "npm version major && npm run build && npm publish --access public"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.27.1",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-backgrounds": "^8.0.0",
    "@storybook/addon-controls": "^8.0.0",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/testing-react": "2.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "stylelint": "^16.21.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  },
  "browserslist": {},
  "files": [
    "dist",
    "src/styles",
    "index.scss"
  ]
}
